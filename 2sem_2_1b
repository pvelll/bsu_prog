#include<iostream>
#include<stdlib.h>
using namespace std;
int main()
{
	srand(time(NULL));
	int n = 0;
	unsigned short int num;
	cout << "Size:";
	cin >> n;
	if (n >= pow(10, 9))
	{
		cerr << "Error";
		return 0;
	}
	int* arr = new int[n];
	for (int i = 0; i < n; i++)
	{
		if (i % 3 == 0)
		{
			num = rand() % 100;
		}
		else if (i % 2)
		{
			num = rand() % 1000;
		}
		else
		{
			num = rand() % 10000;
		}
		if (num <= 10000)
		{
			arr[i] = num;
		}
		else
		{
			cerr << "Error";
			return 0;
		}
	}
	/*cout << "Stock:" << endl;
	for (int i = 0; i < n; i++)
	{
		cout << arr[i] << "\t";
	}
	cout << endl;*/


	for (int j, i = 0; i < n; i++)
	{
		for (j = i + 1; j < n; j++)
		{
			if (arr[i] == arr[j])
				break;
		}
		if (j == n)
		{
			for (j = i; j < n - 1; j++)
			{
				arr[j] = arr[j + 1];
			}
			i--;
			n--;

		}
	}
	for (int i = 0; i < n - 1; i++) {
		for (int j = 0; j < n - i - 1; j++) {
			if (arr[j] > arr[j + 1]) {

				swap(arr[j], arr[j + 1]);

			}
		}
	}

	for (int i = 0; i < n; i++)
	{
		for (int j = i + 1; j < n; j++)
		{
			if (arr[i] == arr[j])
			{
				for (j = i; j < n - 1; j++)
				{
					arr[j] = arr[j + 1];
				}
				i--;
				n--;
			}
		}
	}
	cout << "Final:" << endl;
	for (int i = 0; i < n; i++)
	{
		cout << arr[i] << "\t";
	}
	delete[] arr;

}
